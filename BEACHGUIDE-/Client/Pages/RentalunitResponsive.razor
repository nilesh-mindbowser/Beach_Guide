    }

    .properties-nav {
        text-decoration: none;
        display: flex;
        justify-content: center;
        align-items: center;
        padding: 3px 25px 0px 19px;
        gap: 10px;
        isolation: isolate;
        height: 60px;
        background: linear-gradient(176.06deg, #7CBBD9 3.22%, #469DC9 96.78%);
        border: 1px solid #4199C7;
        flex: none;
        order: 0;
        flex-grow: 1;
    }

    .shores-heading {
        display: flex;
        flex-direction: row;
        align-items: flex-start;
        padding: 24px 69px;
        gap: 10px;
        height: 42px;
        text-decoration: none;
        background: #FFFFFF;
        flex: none;
        order: 3;
        align-self: stretch;
        flex-grow: 0;
    }

    .shores-link {
        width: 405px;
        height: 22px;
        font-family: 'Inter';
        font-style: normal;
        font-weight: 600;
        font-size: 18px;
        line-height: 22px;
        color: #626262;
        flex: none;
        order: 0;
        flex-grow: 0;
    }

    .gulf-hedding {
        display: flex;
        flex-direction: column;
        justify-content: center;
        align-items: center;
        padding: 0px 0px 0px 0px;
        background: #FFFFFF;
        height: 130px;
    }

    .gulf-heading2 {
        display: flex;
        justify-content: center;
        align-items: center;
        flex-direction: column;
    }

    .gulf-heading3 {
        display: flex;
        justify-content: center;
        flex-direction: row;
        align-items: center;
        gap: 10px;
    }

    .gulf-title {
        font-size: 30px;
        margin: 0;
        line-height: 50px;
        text-transform: uppercase;
        font-family: 'Inter';
        font-style: normal;
        font-weight: 700;
        color: #489FCA;
    }

    .gulf-inform {
        font-size: 18px;
        color: #FF8807;
        text-decoration: none;
    }

    .container-body {
        display: flex;
        flex-wrap: wrap;
    }

    .card {
        flex: 0 0 calc(33.33% - 20px);
        background-color: #f2f2f2;
        padding: 20px;
        overflow: hidden;
    }

    .fluid-image {
        width: 100%;
        height: auto;
    }


