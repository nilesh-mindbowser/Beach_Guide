<style>
    .beach-guide {
        height: 241px;
        background-image: url("./Images/pp.png");
        /* background: linear-gradient(0deg, rgba(0, 0, 0, 0.7), rgba(0, 0, 0, 0.7)), url("pp.png");*/
        background-repeat: no-repeat;
        background-size: cover;
        /* Inside auto layout */
        display: flex;
        order: 1;
        flex-grow: 0;
    }

    .beach-guide-box {
        /* Auto layout */
        display: flex;
        flex-direction: column;
        align-items: center;
        padding: 0px;
        gap: 40px;
        position: absolute;
        /* width: 1292px;
        height: 144px;*/
        /* left: 74px;*/
        margin-top: 49px;
        margin-left: 74px;
    }

    .beach-guide-heading {
        /* Auto layout */
        display: flex;
        flex-direction: column;
        justify-content: center;
        align-items: center;
        padding: 0px;
        gap: 16px;
        height: 42px;
        /* Inside auto layout */
        order: 0;
        flex-grow: 0;
        color: white;
    }

    .navslides-box {
        display: flex;
        flex-direction: column;
        align-items: flex-start;
        padding: 30px 0px 0px 68px;
        background: #FFFFFF;
        flex: none;
        order: 2;
        flex-grow: 0;
    }

    .navslides-box1 {
        display: flex;
